<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="mynightout.entity.Cellar" table="cellar">
       <id name="clubId" type="java.lang.Integer">
            <column name="Club_id" />
            <generator class="identity" />
        </id>
       
        <property name="vodka" type="int">
            <column name="Vodka" not-null="true" />
        </property>
        <property name="whiskey" type="int">
            <column name="Whiskey" not-null="true" />
        </property>
        <property name="wine" type="int">
            <column name="Wine" not-null="true" />
        </property>
        <property name="liqueur" type="int">
            <column name="Liqueur" not-null="true" />
        </property>
        <property name="rum" type="int">
            <column name="Rum" not-null="true" />
        </property>
        <property name="tequila" type="int">
            <column name="Tequila" not-null="true" />
        </property>
        <property name="beer" type="int">
            <column name="Beer" not-null="true" />
        </property> 
        <one-to-one name="nightclub" class="mynightout.entity.Nightclub"
			constrained="true"></one-to-one>
  </class>
</hibernate-mapping>
