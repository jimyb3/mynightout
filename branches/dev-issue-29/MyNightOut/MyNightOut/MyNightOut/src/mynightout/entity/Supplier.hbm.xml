<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="mynightout.entity.Supplier" table="supplier">  
        <id name="supplierId" type="java.lang.Integer">
            <column name="Supplier_id" />
            <generator class="identity" />
        </id>
        <property name="firstName" type="string">
            <column name="First_name" length="30" not-null="true" />
        </property>
        <property name="lastName" type="string">
            <column name="Last_name" length="30" not-null="true" />
        </property>
        <property name="companyName" type="string">
            <column name="Company_name" length="40" not-null="true" />
        </property>
        <property name="address" type="string">
            <column name="Address" length="50" not-null="true" />
        </property> 
          <property name="cellPhoneNumber" type="string">
            <column name="Cellphone_num" length="10" not-null="true" />
        </property> 
        <property name="officePhoneNumber" type="string">
            <column name="Officephone_num" length="10" not-null="true" />
        </property>   
        <property name="emailAddress" type="string">
            <column name="Email_address" length="40" not-null="true" />
        </property>     
        <set name="nightclub" table="nightclub" 
        	inverse="false" lazy="true" fetch="select" cascade="all" >
            <key>
                <column name="Supplier_id" not-null="true" />
            </key>
            <many-to-many entity-name="mynightout.entity.Nightclub">
                <column name="Club_id" not-null="true" />
            </many-to-many>
        </set>       
    </class>
</hibernate-mapping>
